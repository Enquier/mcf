{
  "extends": "airbnb-base",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "generators": false,
      "arrowFunctions":true,
      "objectLiteralDuplicateProperties": false,
      "jsx": true
    }
  },

  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "jquery": true
  },

  "globals": {
    "M": false
  },

  "plugins": [
    "jsdoc",
    "security"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "implicit-arrow-linebreak": "warn",
    "no-param-reassign": "warn",
    "class-methods-use-this": "off"
  }
}
